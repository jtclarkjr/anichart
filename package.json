{
  "name": "anichart",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ssr": "bun run dev-unified.ts",
    "dev:ssr:legacy": "bun run dev-ssr.ts",
    "dev:ssr:simple": "PORT=5174 bun run server.ts",
    "pre-build": "run-p test:unit lint prettier:fix",
    "build-only": "vite build",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssr": "bun run build:client && bun run build:server",
    "start": "NODE_ENV=production bun run server-prod.ts",
    "preview:ssr": "bun run build:ssr && bun run start",
    "build": "run-s pre-build build-only",
    "test:unit": "vitest run --environment jsdom --root src/",
    "lint": "run-p lint:es lint:style",
    "lint:es": "eslint ./src",
    "lint:style": "stylelint ./src --fix",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@apollo/client": "^4.0.3",
    "graphql": "^16.11.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@pinia/testing": "^1.0.2",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/bun": "^1.2.21",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.34.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.1",
    "sass": "^1.89.1",
    "start-server-and-test": "^2.0.10",
    "stylelint": "^16.19.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-scss": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
